{
  "name": "formulatex",
  "version": "0.1.1",
  "description": "AI based tool that can automatically convert formula into LaTeX code & other features.",
  "keywords": [
    "math",
    "formula",
    "AI",
    "LaTeX"
  ],
  "homepage": "https://github.com/Leo204-LKY/FormuLaTeX#readme",
  "bugs": {
    "url": "https://github.com/Leo204-LKY/FormuLaTeX/issues"
  },
  "repository": "git+https://github.com/Leo204-LKY/FormuLaTeX.git",
  "license": "GPL-3.0-only",
  "author": "Group Kappa (UoA JC3510)",
  "type": "commonjs",
  "main": "dist/main/main.js",
  "prisma": {
    "schema": "src/server/database/schema/schema.prisma"
  },
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "concurrently \"yarn dev:main\" \"yarn dev:renderer\"",
    "dev:main": "tsc --watch",
    "dev:renderer": "cd src/renderer && yarn dev",
    "start": "yarn build && electron .",
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "tsc",
    "build:renderer": "cd src/renderer && yarn build",
    "package": "yarn build && electron-builder",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "jest --config jest.config.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.9.0",
    "node-machine-id": "^1.1.12",
    "openai": "^4.97.0"
  },
  "devDependencies": {
    "@electron/typescript-definitions": "^9.1.2",
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.25.0",
    "@eslint/json": "^0.12.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/tailwindcss": "^3.1.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "concurrently": "^8.0.1",
    "copyfiles": "^2.4.1",
    "dompurify": "^3.2.6",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "markdown-it": "^14.1.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mathjax3": "^4.3.2",
    "marked": "^15.0.12",
    "mathjax": "^3.2.2",
    "prettier": "3.5.3",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.6.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.2",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^2.2.10"
  }
}
